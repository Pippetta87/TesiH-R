@article{sekiya2003were,
  title={Were planetesimals formed by dust accretion in the solar nebula?},
  author={Sekiya, Minoru and Takeda, Hidenori},
  journal={Earth, planets and space},
  volume={55},
  number={5},
  pages={263--269},
  year={2003},
  publisher={The Seismological Society of Japan, Society of Geomagnetism and Earth, Planetary and Space Sciences, The Volcanological Society of Japan, The Geodetic Society of Japan, The Japanese Society for Planetary Sciences},
  keywords={primary,planetesimal,dust accretion},
  addendum={The growth of meter-sized bodies in the solar nebula by dust accretion is examined. The meter-sized bodies have velocity about $50 m/s$ relative to the gas and small dust aggregates. When a small dust aggregate hits a meter-sized body, the aggregate breaks into dust monomers.  These monomers accrete onto the body after several bouncing as proposed by Wurm et al., Icarus
(2001), if the mean free path of the gas molecules is larger than the radius of the body. On the other hand, the monomers never hit the surface of the body again, if the body is much larger than the mean free path of the molecules. The sizes of bodies would be limited to the order of 10 times the mean free path. Kilometer-sized planetesimals were hardly formed by dust accretion in the region within 5 AU from the sun where the mean free path is less than 1 m.  The planetesimals were probably formed by the gravitational instabilities in this region.}
}


@article{ruden1991dynamical,
  title={The dynamical evolution of the protosolar nebula},
  author={Ruden, Steven P and Pollack, James B},
  journal={The Astrophysical Journal},
  volume={375},
  pages={740--760},
  year={1991},
  keywords={nebula,disk,protosolar nebula,dynamical evolution,primary},
  addendum={We calculate the global evolution of protostellar nebulae for a variety of initial disk masses and angular momenta. Although we approximate the viscous stress by an ''alpha'' model, we calibrate the constant a in terms of the properties of turbulent thermal convection. As the evolution proceeds, the nebula viscously spreads and is partially accreted by the protostar. The consequent decrease of the surface density renders portions of the disk optically thin in the vertical direction. When the Rosseland mean optical depth decreases below a critical value of order unity, we assume that the disk is unable locally to sustain thermal convection and that the turbulent viscosity and energy dissipation cease.}
}


