@article{papaloizou1999critical,
  title={Critical protoplanetary core masses in protoplanetary disks and the formation of short-period giant planets},
  author={Papaloizou, John CB and Terquem, Caroline},
  journal={The Astrophysical Journal},
  volume={521},
  number={2},
  pages={823},
  year={1999},
  publisher={IOP Publishing},
keywords={rev,accretion,formation,short-period,planetary systems},
addendum={We study a solid protoplanetary core undergoing radial migration in a protoplanetary disk. We consider cores in the mass range $\approx 1 − 10 \mearth{}$ embedded in a gaseous protoplanetary disk at different radial locations. We suppose the core luminosity is generated as a result of planetesimal accretion and calculate the structure of the gaseous envelope assuming hydrostatic and thermal equilibrium. This is a good approximation during the early growth of the core while its mass is less than the critical value, $M_c$, above which such static solutions can no longer be obtained and rapid gas accretion begins. The critical value corresponds to the crossover mass above which rapid gas accretion begins in time dependent calculations. We model the structure and evolution of the protoplanetary nebula as an accretion disk with constant . We present analytic ts for the steady state relation between disk surface density and mass accretion rate as a function of radius. We calculate $M_c$ as a function of radial location, gas accretion rate through the disk, and planetesimal accretion rate onto the core. For a fixed planetesimal accretion rate, $M_c$ is found to increase inwards. On the other hand it decreases with the planetesimal accretion rate and hence the core luminosity.}
}

